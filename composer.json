{
    "name": "plazari15/mail-tracker",
    "type": "library",
    "description": "This is an Fork from 'jdavidbakr' this library show logs from emails sents",
    "keywords": [
        "plazari15",
        "MailTracker"
    ],
    "homepage": "https://github.com/plazari15/mail-tracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Lazari",
            "email": "plazari96@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": ">=5.3.23",
        "guzzlehttp/guzzle": "^5.3.1|^6.2.1",
        "illuminate/support": "~5.1",
        "aws/aws-sdk-php-laravel": "~3.0",
        "aws/aws-php-sns-message-validator": "^1.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "orchestra/testbench-browser-kit": "~3.0"
    },
    "suggest": {
        "fedeisas/laravel-mail-css-inliner": "Automatically inlines CSS into all outgoing mail."
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\MailTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jdavidbakr\\MailTracker\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
